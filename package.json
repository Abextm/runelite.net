    "react-snap": "^1.12.0",
    "standard": "^11.0.1",
    "surge": "^0.20.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "prebuild": "standard",
    "build": "react-scripts build",
    "postbuild": "node generate-feed.js && react-snap",
    "eject": "react-scripts eject",
    "deploy": "npm run build && surge -p build"
  },
  "reactSnap": {
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox"
    ],
    "crawl": false,
    "include": [
      "/",
      "/blog",
      "/features"
    ]
  },
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions"
    ],
    "production": [
      ">1%",
      "last 4 versions",
      "Firefox ESR",
      "not ie < 11"
    ]
  }
}
