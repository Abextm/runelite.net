    "react-redux": "^5.0.7",
    "react-redux-loading-bar": "^4.0.5",
    "react-timeago": "^4.1.9",
    "reactstrap": "^6.4.0",
    "redux": "^4.0.0",
    "redux-actions": "^2.6.1",
    "redux-first-router": "0.0.16-next",
    "redux-first-router-link": "^1.4.2",
    "redux-routines": "^3.0.0",
    "reselect": "^3.0.1"
  },
  "devDependencies": {
    "front-matter": "^2.3.0",
    "jstoxml": "^1.4.3",
    "markdown-it": "^8.4.2",
    "markdown-with-front-matter-loader": "^0.1.0",
